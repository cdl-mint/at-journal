<?xml version="1.0" encoding="UTF-8"?>
<tsUML:UMLModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:tsUML="tsUML">
  <robot speedTELEMETRY="5"
      name="1"/>
  <robot name="2"/>
  <sm Class="//@robot.0"
      name="robot1">
    <state isActiveTELEMETRY="true"
        isStart="true"
        name="S1"/>
    <state name="S2"/>
    <transition
        successor="//@sm.0/@state.1"
        predecessor="//@sm.0/@state.0"
        name="T1"/>
  </sm>
  <sm Class="//@robot.1"
      name="alternative">
    <state name="x"/>
    <state name="y"/>
  </sm>
  <robotstatemachine
      class="//@robot.0"
      name="robot1">
    <s1 name="s1"/>
    <s2 name="s2"/>
    <t1 name="t1"
        src="//@robotstatemachine.0/@s1"
        trg="//@robotstatemachine.0/@s2"/>
  </robotstatemachine>
  <robotstatemachine
      class="//@robot.1"
      name="robot2">
    <s1/>
    <s2/>
    <t1 src="//@robotstatemachine.1/@s1"
        trg="//@robotstatemachine.1/@s2"/>
  </robotstatemachine>
</tsUML:UMLModel>
